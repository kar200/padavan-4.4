/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "D-Link DIR-853 A3";

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	leds {
		compatible = "gpio-leds";

		sys_blue {
			label = "power";
			gpios = <&gpio0 14 GPIO_ACTIVE_HIGH>;
		};

		wan {
			label = "wan";
			gpios = <&gpio0 16 GPIO_ACTIVE_HIGH>;
		};
    
		wlan5g {
			label = "sw5g";
			gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1radio";
		};

		wlan2g {
			label = "sw2g";
			gpios = <&gpio0 4 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0radio";
		};

		usb {
			label = "usb";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "usbport";
		};
	};
	
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
		
		wps {
			label = "wps";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		fn1 { /* wlan */
			label = "fn1";
			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_FN_1>;
		};
	};
};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0 0x80000>; /* 512k */
			};
						
			partition@80000 {
				label = "Config";
				reg = <0x80000 0x80000>; /* 512k */
			};

			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x40000>; /* 256k */
			};

			partition@140000 {
				label = "config2";
				reg = <0x140000 0x40000>; /* 256k */
			};

			partition@180000 {
				label = "firmware";
				reg = <0x180000 0x1800000>; /* 24M */
			};

				partition@1980000 {
				label = "Storage";
				reg = <0x1980000 0x1000000>; /* 4096K */
			};
			
			partition@2980000 {
				label = "RWFS";
				reg = <0x2980000 0x2000000>; /* 4096K */
			};

			partition@all {
				label = "ALL";
				reg = <0x0 0x7f80000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&xhci {
	status = "okay";
};

&u3phy1 {
	status = "okay";
};
